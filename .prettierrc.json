{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 160,
  "endOfLine": "lf",
  "insertPragma": false,
  "requirePragma": false,
  "plugins": [],
  "overrides": [
    {
      "files": ["**/*.y*ml"],
      "options": {
        "quoteProps": "preserve",
        "singleQuote": true,
        "tabWidth": 2,
        "useTabs": false,
        "bracketSpacing": false,
        "bracketSameLine": true
      }
    },
    {
      "files": ["*.md"],
      "options": {
        "quoteProps": "preserve",
        "singleQuote": false,
        "tabWidth": 2
      }
    },
    {
      "files": ["*.json*"],
      "options": {
        "quoteProps": "preserve",
        "singleQuote": false,
        "tabWidth": 2
      }
    }
  ]
}
